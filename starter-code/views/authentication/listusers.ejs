<div class="container">
  <div class="row">

    <h3>Wiew Users</h3>
    <h4>List of registered users:</h4>
    <% users.forEach((user,i) => { %>
    <div class="col-md-4">
      <div class="post">
        User number:
        <%=i+1%>
        <br>
        User name: <strong>
          <%=user.username %></strong>
        <br>
        <img class="avatar" src="<%=user.profile_pic.pic_path %>" alt="">

      </div>
    </div>
    <% }) %>

  </div>
</div>
